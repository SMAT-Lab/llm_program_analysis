{
    "AlbumBarModel": [],
    "AlbumBarModel.createActionBar": [
        "screenManager.isHorizontal",
        "AlbumBarModel.updateHorizontalActionBar",
        "AlbumBarModel.updateVerticalActionBar"
    ],
    "AlbumBarModel.updateActionBar": [
        "screenManager.isHorizontal",
        "AlbumBarModel.updateHorizontalActionBar",
        "AlbumBarModel.updateVerticalActionBar"
    ],
    "AlbumBarModel.updateHorizontalActionBar": [
        "screenManager.isSidebar",
        "$r",
        "AlbumBarModel.getMenuList",
        "Action.CANCEL",
        "ActionBarMode.SELECTION_MODE",
        "ActionBarSelectionMode.MULTI",
        "Action.NEW",
        "ActionBarMode.STANDARD_MODE"
    ],
    "AlbumBarModel.updateVerticalActionBar": [
        "Action.NEW",
        "screenManager.isSidebar",
        "$r",
        "Action.CANCEL",
        "ActionBarMode.SELECTION_MODE",
        "ActionBarSelectionMode.MULTI",
        "ActionBarMode.STANDARD_MODE"
    ],
    "AlbumBarModel.getMenuList": [
        "Action.RENAME",
        "Action.RENAME_INVALID",
        "Action.DELETE",
        "Action.DELETE_INVALID"
    ]
}