          "id": 1,
          "start_line": 1,
          "end_line": 6,
          "label": "def get_valid_pos(position: tuple[int, int], n: int) -> list[tuple[int, int]]:\n    \"\"\"\n    Find all the valid positions a knight can move to from the current position.\n    >>> get_valid_pos((1, 3), 4)\n    [(2, 1), (0, 1), (3, 2)]\n    \"\"\"",
          "successors": [
            {
              "id": 2,
              "start_line": 7,
              "end_line": 7,
              "label": "y, x = position",
              "successors": [
                {
                  "id": 3,
                  "start_line": 8,
                  "end_line": 17,
                  "label": "positions = [\n        (y + 1, x + 2),\n        (y - 1, x + 2),\n        (y + 1, x - 2),\n        (y - 1, x - 2),\n        (y + 2, x + 1),\n        (y + 2, x - 1),\n        (y - 2, x + 1),\n        (y - 2, x - 1),\n    ]",
                  "successors": [
                    {
                      "id": 4,
                      "start_line": 18,
                      "end_line": 18,
                      "label": "permissible_positions = []",
                      "successors": [
                        {
                          "id": 5,
                          "start_line": 19,
                          "end_line": 19,
                          "label": "for inner_position in positions:",
                          "successors": [
                            {
                              "id": 6,
                              "start_line": 20,
                              "end_line": 20,
                              "label": "y_test, x_test = inner_position",
                              "successors": [
                                {
                                  "id": 7,
                                  "start_line": 21,
                                  "end_line": 21,
                                  "label": "if 0 <= y_test < n and 0 <= x_test < n:",
                                  "successors": [
                                    {
                                      "id": 8,
                                      "start_line": 22,
                                      "end_line": 22,
                                      "label": "permissible_positions.append(inner_position)",
                                      "successors": []
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
