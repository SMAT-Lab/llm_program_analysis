    {
      "name": "is_complete",
      "type": "CFG",
      "blocks": [
        {
          "id": 1,
          "start_line": 1,
          "end_line": 8,
          "label": "def is_complete(board: list[list[int]]) -> bool:\n    \"\"\"\n    Check if the board (matrix) has been completely filled with non-zero values.\n    >>> is_complete([[1]])\n    True\n    >>> is_complete([[1, 2], [3, 0]])\n    False\n    \"\"\"",
          "successors": [
            {
              "id": 2,
              "start_line": 9,
              "end_line": 9,
              "label": "return not any(elem == 0 for row in board for elem in row)",
              "successors": []
            }
          ]
        }
      ],
