              "id": 1,
              "start_line": 1,
              "end_line": 3,
              "label": "def run(\n        self, input_data: Input, *, credentials: JinaCredentials, **kwargs\n    ) -> BlockOutput:",
              "successors": [
                {
                  "id": 2,
                  "start_line": 4,
                  "end_line": 4,
                  "label": "url = \"https://api.jina.ai/v1/embeddings\"",
                  "successors": [
                    {
                      "id": 3,
                      "start_line": 5,
                      "end_line": 8,
                      "label": "headers = {\n            \"Content-Type\": \"application/json\",\n            \"Authorization\": f\"Bearer {credentials.api_key.get_secret_value()}\",\n        }",
                      "successors": [
                        {
                          "id": 4,
                          "start_line": 9,
                          "end_line": 9,
                          "label": "data = {\"input\": input_data.texts, \"model\": input_data.model}",
                          "successors": [
                            {
                              "id": 5,
                              "start_line": 10,
                              "end_line": 10,
                              "label": "response = requests.post(url, headers=headers, json=data)",
                              "successors": [
                                {
                                  "id": 6,
                                  "start_line": 11,
                                  "end_line": 11,
                                  "label": "embeddings = [e[\"embedding\"] for e in response.json()[\"data\"]]",
                                  "successors": [
                                    {
                                      "id": 7,
