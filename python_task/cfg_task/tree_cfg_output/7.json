              "label": "async def get_or_refill_credit(self, user_id: str) -> int:",
              "successors": [
                {
                  "id": 2,
                  "start_line": 2,
                  "end_line": 8,
                  "label": "cur_time = self.time_now()\n        cur_month = cur_time.replace(day=1, hour=0, minute=0, second=0, microsecond=0)\n        nxt_month = (\n            cur_month.replace(month=cur_month.month + 1)\n            if cur_month.month < 12\n            else cur_month.replace(year=cur_month.year + 1, month=1)\n        )",
                  "successors": [
                    {
                      "id": 3,
                      "start_line": 9,
                      "end_line": 17,
                      "label": "user_credit = await CreditTransaction.prisma().group_by(\n            by=[\"userId\"],\n            sum={\"amount\": True},\n            where={\n                \"userId\": user_id,\n                \"createdAt\": {\"gte\": cur_month, \"lt\": nxt_month},\n                \"isActive\": True,\n            },\n        )",
                      "successors": [
                        {
                          "id": 4,
                          "start_line": 18,
                          "end_line": 18,
                          "label": "if user_credit:",
                          "successors": [
                            {
                              "id": 5,
                              "start_line": 19,
                              "end_line": 20,
                              "label": "credit_sum = user_credit[0].get(\"_sum\") or {}\n            return credit_sum.get(\"amount\", 0)",
                              "successors": []
                            },
                            {
                              "id": 6,
                              "start_line": 21,
                              "end_line": 21,
                              "label": "key = f\"MONTHLY-CREDIT-TOP-UP-{cur_month}\"",
                              "successors": [
                                {
                                  "id": 7,
                                  "start_line": 22,
                                  "end_line": 22,
                                  "label": "try:",
                                  "successors": [
                                    {
                                      "id": 8,
                                      "start_line": 23,
                                      "end_line": 31,
                                      "label": "await CreditTransaction.prisma().create(\n                data={\n                    \"amount\": self.num_user_credits_refill,\n                    \"type\": CreditTransactionType.TOP_UP,\n                    \"userId\": user_id,\n                    \"transactionKey\": key,\n                    \"createdAt\": self.time_now(),\n                }\n            )",
                                      "successors": [
                                        {
                                          "id": 9,
                                          "start_line": 32,
                                          "end_line": 33,
                                          "label": "except UniqueViolationError:\n            pass  # Already refilled this month",
