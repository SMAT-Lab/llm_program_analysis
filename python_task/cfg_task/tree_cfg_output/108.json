              "successors": [
                {
                  "id": 2,
                  "start_line": 2,
                  "end_line": 8,
                  "label": "params = {\n            \"client_id\": self.client_id,\n            \"redirect_uri\": self.redirect_uri,\n            \"response_type\": \"code\",\n            \"owner\": \"user\",\n            \"state\": state,\n        }",
                  "successors": [
                    {
                      "id": 3,
                      "start_line": 9,
                      "end_line": 9,
                      "label": "return f\"{self.auth_base_url}?{urlencode(params)}\"",
