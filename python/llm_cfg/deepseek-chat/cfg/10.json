{
    "nodes": [
        {
            "id": "chunk_0_GlobalBlock_1",
            "code": "import json\nimport os\nimport requests\nimport sys\nimport time\nfrom typing import Dict, List, Tuple\n"
        },
        {
            "id": "chunk_0_GlobalBlock_2",
            "code": "CHECK_INTERVAL = 30\n"
        },
        {
            "id": "chunk_1_get_environment_variables_1",
            "code": "def get_environment_variables() -> Tuple[str, str, str, str, str]:\n    \"\"\"Retrieve and return necessary environment variables.\"\"\"\n"
        },
        {
            "id": "chunk_1_get_environment_variables_2",
            "code": "    try:\n        with open(os.environ[\"GITHUB_EVENT_PATH\"]) as f:\n            event = json.load(f)\n"
        },
        {
            "id": "chunk_1_get_environment_variables_3",
            "code": "\n"
        },
        {
            "id": "chunk_1_get_environment_variables_4",
            "code": "        # Handle both PR and merge group events\n"
        },
        {
            "id": "chunk_1_get_environment_variables_5",
            "code": "        if \"pull_request\" in event:\n            sha = event[\"pull_request\"][\"head\"][\"sha\"]\n"
        },
        {
            "id": "chunk_1_get_environment_variables_6",
            "code": "        else:\n            sha = os.environ[\"GITHUB_SHA\"]\n"
        },
        {
            "id": "chunk_1_get_environment_variables_7",
            "code": "\n"
        },
        {
            "id": "chunk_1_get_environment_variables_8",
            "code": "        return (\n            os.environ[\"GITHUB_API_URL\"],\n            os.environ[\"GITHUB_REPOSITORY\"],\n            sha,\n            os.environ[\"GITHUB_TOKEN\"],\n            os.environ[\"GITHUB_RUN_ID\"],\n        )\n"
        },
        {
            "id": "chunk_1_get_environment_variables_9",
            "code": "    except KeyError as e:\n        print(f\"Error: Missing required environment variable or event data: {e}\")\n        sys.exit(1)\n"
        },
        {
            "id": "chunk_1_get_environment_variables_10",
            "code": "\n\n"
        },
        {
            "id": "chunk_2_make_api_request_1",
            "code": "def make_api_request(url: str, headers: Dict[str, str]) -> Dict:\n    \"\"\"Make an API request and return the JSON response.\"\"\"\n"
        },
        {
            "id": "chunk_2_make_api_request_2",
            "code": "    try:\n        print(\"Making API request to:\", url)\n        response = requests.get(url, headers=headers, timeout=10)\n        response.raise_for_status()\n        return response.json()\n"
        },
        {
            "id": "chunk_2_make_api_request_3",
            "code": "    except requests.RequestException as e:\n        print(f\"Error: API request failed. {e}\")\n        sys.exit(1)\n"
        },
        {
            "id": "chunk_3_process_check_runs_1",
            "code": "def process_check_runs(check_runs: List[Dict]) -> Tuple[bool, bool]:\n    \"\"\"Process check runs and return their status.\"\"\"\n    runs_in_progress = False\n    all_others_passed = True\n\n"
        },
        {
            "id": "chunk_3_process_check_runs_2",
            "code": "    for run in check_runs:\n        if str(run[\"name\"]) != \"Check PR Status\":\n"
        },
        {
            "id": "chunk_3_process_check_runs_3",
            "code": "            status = run[\"status\"]\n            conclusion = run[\"conclusion\"]\n\n"
        },
        {
            "id": "chunk_3_process_check_runs_4",
            "code": "            if status == \"completed\":\n                if conclusion not in [\"success\", \"skipped\", \"neutral\"]:\n"
        },
        {
            "id": "chunk_3_process_check_runs_5",
            "code": "                    all_others_passed = False\n                    print(\n                        f\"Check run {run['name']} (ID: {run['id']}) has conclusion: {conclusion}\"\n                    )\n"
        },
        {
            "id": "chunk_3_process_check_runs_6",
            "code": "            else:\n                runs_in_progress = True\n                print(f\"Check run {run['name']} (ID: {run['id']}) is still {status}.\")\n                all_others_passed = False\n"
        },
        {
            "id": "chunk_3_process_check_runs_7",
            "code": "        else:\n            print(\n                f\"Skipping check run {run['name']} (ID: {run['id']}) as it is the current run.\"\n            )\n"
        },
        {
            "id": "chunk_3_process_check_runs_8",
            "code": "\n    return runs_in_progress, all_others_passed\n"
        },
        {
            "id": "chunk_4_main_1",
            "code": "def main():\n    api_url, repo, sha, github_token, current_run_id = get_environment_variables()\n"
        },
        {
            "id": "chunk_4_main_2",
            "code": "    endpoint = f\"{api_url}/repos/{repo}/commits/{sha}/check-runs\"\n    headers = {\n        \"Accept\": \"application/vnd.github.v3+json\",\n    }\n"
        },
        {
            "id": "chunk_4_main_3",
            "code": "    if github_token:\n        headers[\"Authorization\"] = f\"token {github_token}\"\n"
        },
        {
            "id": "chunk_4_main_4",
            "code": "    print(f\"Current run ID: {current_run_id}\")\n"
        },
        {
            "id": "chunk_4_main_5",
            "code": "    while True:\n        data = make_api_request(endpoint, headers)\n"
        },
        {
            "id": "chunk_4_main_6",
            "code": "        check_runs = data[\"check_runs\"]\n"
        },
        {
            "id": "chunk_4_main_7",
            "code": "        print(\"Processing check runs...\")\n"
        },
        {
            "id": "chunk_4_main_8",
            "code": "        print(check_runs)\n"
        },
        {
            "id": "chunk_4_main_9",
            "code": "        runs_in_progress, all_others_passed = process_check_runs(check_runs)\n"
        },
        {
            "id": "chunk_4_main_10",
            "code": "        if not runs_in_progress:\n            break\n"
        },
        {
            "id": "chunk_4_main_11",
            "code": "        print(\n            \"Some check runs are still in progress. \"\n            f\"Waiting {CHECK_INTERVAL} seconds before checking again...\"\n        )\n"
        },
        {
            "id": "chunk_4_main_12",
            "code": "        time.sleep(CHECK_INTERVAL)\n"
        },
        {
            "id": "chunk_4_main_13",
            "code": "    if all_others_passed:\n        print(\"All other completed check runs have passed. This check passes.\")\n        sys.exit(0)\n"
        },
        {
            "id": "chunk_4_main_14",
            "code": "    else:\n        print(\"Some check runs have failed or have not completed. This check fails.\")\n        sys.exit(1)\n"
        },
        {
            "id": "chunk_5_GlobalBlock_1",
            "code": "if __name__ == \"__main__\":\n    main()\n"
        }
    ],
