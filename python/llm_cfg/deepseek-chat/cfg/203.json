{
    "nodes": [
        {
            "id": "chunk_0_GlobalBlock_1",
            "code": "import contextlib\nimport logging.config\nimport os\n\nimport dotenv\nimport fastapi\nimport fastapi.middleware.cors\nimport fastapi.middleware.gzip\nimport prisma\nimport prometheus_fastapi_instrumentator\nimport sentry_sdk\nimport sentry_sdk.integrations.asyncio\nimport sentry_sdk.integrations.fastapi\nimport sentry_sdk.integrations.starlette\n\nimport market.config\nimport market.routes.admin\nimport market.routes.agents\nimport market.routes.analytics\nimport market.routes.search\nimport market.routes.submissions\n\n"
        },
        {
            "id": "chunk_0_GlobalBlock_2",
            "code": "dotenv.load_dotenv()\n"
        },
        {
            "id": "chunk_0_GlobalBlock_3",
            "code": "logging.config.dictConfig(market.config.LogConfig().model_dump())\n"
        },
        {
            "id": "chunk_0_GlobalBlock_4",
            "code": "if os.environ.get(\"SENTRY_DSN\"):\n    sentry_sdk.init(\n        dsn=os.environ.get(\"SENTRY_DSN\"),\n        traces_sample_rate=1.0,\n        profiles_sample_rate=1.0,\n        enable_tracing=True,\n        environment=os.environ.get(\"RUN_ENV\", default=\"CLOUD\").lower(),\n        integrations=[\n            sentry_sdk.integrations.starlette.StarletteIntegration(\n                transaction_style=\"url\"\n            ),\n            sentry_sdk.integrations.fastapi.FastApiIntegration(transaction_style=\"url\"),\n            sentry_sdk.integrations.asyncio.AsyncioIntegration(),\n        ],\n    )\n"
        },
        {
            "id": "chunk_0_GlobalBlock_5",
            "code": "db_client = prisma.Prisma(auto_register=True)\n"
        },
        {
            "id": "chunk_1_lifespan_1",
            "code": "@contextlib.asynccontextmanager\nasync def lifespan(app: fastapi.FastAPI):\n"
        },
        {
            "id": "chunk_1_lifespan_2",
            "code": "    await db_client.connect()\n"
        },
        {
            "id": "chunk_1_lifespan_3",
            "code": "    yield\n"
        },
        {
            "id": "chunk_1_lifespan_4",
            "code": "    await db_client.disconnect()\n"
        },
        {
            "id": "chunk_2_GlobalBlock_1",
            "code": "docs_url = \"/docs\"\n"
        },
        {
            "id": "chunk_2_GlobalBlock_2",
            "code": "app = fastapi.FastAPI(\n    title=\"Marketplace API\",\n    description=\"AutoGPT Marketplace API is a service that allows users to share AI agents.\",\n    summary=\"Maketplace API\",\n    version=\"0.1\",\n    lifespan=lifespan,\n    root_path=\"/api/v1/market\",\n    docs_url=docs_url,\n)\n"
        },
        {
            "id": "chunk_2_GlobalBlock_3",
            "code": "app.add_middleware(fastapi.middleware.gzip.GZipMiddleware, minimum_size=1000)\n"
        },
        {
            "id": "chunk_2_GlobalBlock_4",
            "code": "app.add_middleware(\n    middleware_class=fastapi.middleware.cors.CORSMiddleware,\n    allow_origins=os.environ.get(\n        \"BACKEND_CORS_ALLOW_ORIGINS\", \"http://localhost:3000,http://127.0.0.1:3000\"\n    ).split(\",\"),\n    allow_credentials=True,\n    allow_methods=[\"*\"],\n    allow_headers=[\"*\"],\n)\n"
        },
        {
            "id": "chunk_2_GlobalBlock_5",
            "code": "app.include_router(market.routes.agents.router, tags=[\"agents\"])\n"
        },
        {
            "id": "chunk_2_GlobalBlock_6",
            "code": "app.include_router(market.routes.search.router, tags=[\"search\"])\n"
        },
        {
            "id": "chunk_2_GlobalBlock_7",
            "code": "app.include_router(market.routes.submissions.router, tags=[\"submissions\"])\n"
        },
        {
            "id": "chunk_2_GlobalBlock_8",
            "code": "app.include_router(market.routes.admin.router, prefix=\"/admin\", tags=[\"admin\"])\n"
        },
        {
            "id": "chunk_2_GlobalBlock_9",
            "code": "app.include_router(\n    market.routes.analytics.router, prefix=\"/analytics\", tags=[\"analytics\"]\n)\n"
        },
        {
            "id": "chunk_3_health_1",
            "code": "@app.get(\"/health\")\ndef health():\n    return fastapi.responses.HTMLResponse(\n        content=\"<h1>Marketplace API</h1>\", status_code=200\n    )\n"
        },
        {
            "id": "chunk_4_default_1",
            "code": "@app.get(\"/\")\ndef default():\n    return fastapi.responses.HTMLResponse(\n        content=\"<h1>Marketplace API</h1>\", status_code=200\n    )\n"
        },
        {
            "id": "chunk_5_GlobalBlock_1",
            "code": "prometheus_fastapi_instrumentator.Instrumentator().instrument(app).expose(app)\n"
        }
    ],
