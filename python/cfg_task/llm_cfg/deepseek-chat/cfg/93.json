{
    "nodes": [
        {
            "id": "chunk_0_GlobalBlock_1",
            "code": "# https://www.geeksforgeeks.org/solve-crossword-puzzle/\n"
        },
        {
            "id": "chunk_0_GlobalBlock_2",
            "code": "\n\n\n"
        },
        {
            "id": "chunk_1_is_valid_1",
            "code": "def is_valid(\n    puzzle: list[list[str]], word: str, row: int, col: int, vertical: bool\n) -> bool:\n"
        },
        {
            "id": "chunk_1_is_valid_2",
            "code": "    \"\"\"\n    Check if a word can be placed at the given position.\n\n    >>> puzzle = [\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', '']\n    ... ]\n    >>> is_valid(puzzle, 'word', 0, 0, True)\n    True\n    >>> puzzle = [\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', '']\n    ... ]\n    >>> is_valid(puzzle, 'word', 0, 0, False)\n    True\n    \"\"\"\n"
        },
        {
            "id": "chunk_1_is_valid_3",
            "code": "    for i in range(len(word)):\n        if vertical:\n            if row + i >= len(puzzle) or puzzle[row + i][col] != \"\":\n                return False\n        elif col + i >= len(puzzle[0]) or puzzle[row][col + i] != \"\":\n            return False\n    return True\n"
        },
        {
            "id": "chunk_2_GlobalBlock_1",
            "code": "\n\n\n"
        },
        {
            "id": "chunk_3_place_word_1",
            "code": "def place_word(\n    puzzle: list[list[str]], word: str, row: int, col: int, vertical: bool\n) -> None:\n"
        },
        {
            "id": "chunk_3_place_word_2",
            "code": "    \"\"\"\n    Place a word at the given position.\n\n    >>> puzzle = [\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', '']\n    ... ]\n    >>> place_word(puzzle, 'word', 0, 0, True)\n    >>> puzzle\n    [['w', '', '', ''], ['o', '', '', ''], ['r', '', '', ''], ['d', '', '', '']]\n    \"\"\"\n"
        },
        {
            "id": "chunk_3_place_word_3",
            "code": "    for i, char in enumerate(word):\n        if vertical:\n            puzzle[row + i][col] = char\n        else:\n            puzzle[row][col + i] = char\n"
        },
        {
            "id": "chunk_3_place_word_4",
            "code": "\n\n"
        },
        {
            "id": "chunk_4_GlobalBlock_1",
            "code": "def remove_word(\n"
        },
        {
            "id": "chunk_5_remove_word_1",
            "code": "    puzzle: list[list[str]], word: str, row: int, col: int, vertical: bool\n) -> None:\n"
        },
        {
            "id": "chunk_5_remove_word_2",
            "code": "    \"\"\"\n    Remove a word from the given position.\n\n    >>> puzzle = [\n    ...     ['w', '', '', ''],\n    ...     ['o', '', '', ''],\n    ...     ['r', '', '', ''],\n    ...     ['d', '', '', '']\n    ... ]\n    >>> remove_word(puzzle, 'word', 0, 0, True)\n    >>> puzzle\n    [['', '', '', ''], ['', '', '', ''], ['', '', '', ''], ['', '', '', '']]\n    \"\"\"\n"
        },
        {
            "id": "chunk_5_remove_word_3",
            "code": "    for i in range(len(word)):\n        if vertical:\n            puzzle[row + i][col] = \"\"\n        else:\n            puzzle[row][col + i] = \"\"\n\n"
        },
        {
            "id": "chunk_6_GlobalBlock_1",
            "code": "\n\n\n"
        },
        {
            "id": "chunk_7_solve_crossword_1",
            "code": "def solve_crossword(puzzle: list[list[str]], words: list[str]) -> bool:\n    \"\"\"\n    Solve the crossword puzzle using backtracking.\n\n    >>> puzzle = [\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', '']\n    ... ]\n\n    >>> words = ['word', 'four', 'more', 'last']\n    >>> solve_crossword(puzzle, words)\n    True\n    >>> puzzle = [\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', ''],\n    ...     ['', '', '', '']\n    ... ]\n    >>> words = ['word', 'four', 'more', 'paragraphs']\n    >>> solve_crossword(puzzle, words)\n    False\n    \"\"\"\n"
        },
        {
            "id": "chunk_7_solve_crossword_2",
            "code": "    for row in range(len(puzzle)):\n        for col in range(len(puzzle[0])):\n"
        },
        {
            "id": "chunk_7_solve_crossword_3",
            "code": "            if puzzle[row][col] == \"\":\n"
        },
        {
            "id": "chunk_7_solve_crossword_4",
            "code": "                for word in words:\n                    for vertical in [True, False]:\n"
        },
        {
            "id": "chunk_7_solve_crossword_5",
            "code": "                        if is_valid(puzzle, word, row, col, vertical):\n"
        },
        {
            "id": "chunk_7_solve_crossword_6",
            "code": "                            place_word(puzzle, word, row, col, vertical)\n                            words.remove(word)\n                            if solve_crossword(puzzle, words):\n                                return True\n                            words.append(word)\n                            remove_word(puzzle, word, row, col, vertical)\n"
        },
        {
            "id": "chunk_7_solve_crossword_7",
            "code": "                return False\n"
        },
        {
            "id": "chunk_7_solve_crossword_8",
            "code": "    return True\n"
        },
        {
            "id": "chunk_8_GlobalBlock_1",
            "code": "\n\n\n"
        },
        {
            "id": "chunk_9_MainBlock_1",
            "code": "if __name__ == \"__main__\":\n"
        },
        {
            "id": "chunk_9_MainBlock_2",
            "code": "    PUZZLE = [[\"\"] * 3 for _ in range(3)]\n    WORDS = [\"cat\", \"dog\", \"car\"]\n\n"
        },
        {
            "id": "chunk_9_MainBlock_3",
            "code": "    if solve_crossword(PUZZLE, WORDS):\n"
        },
        {
            "id": "chunk_9_MainBlock_4",
            "code": "        print(\"Solution found:\")\n        for row in PUZZLE:\n            print(\" \".join(row))\n"
        },
        {
            "id": "chunk_9_MainBlock_5",
            "code": "    else:\n        print(\"No solution found:\")\n"
        },
        {
            "id": "chunk_9_MainBlock_6",
