            "id": "chunk_0_GlobalBlock_1",
            "code": "from pydantic import BaseModel\n"
        },
        {
            "id": "chunk_0_GlobalBlock_2",
            "code": "\n\n\n"
        },
        {
            "id": "chunk_1_LogConfig_1",
            "code": "class LogConfig(BaseModel):\n    \"\"\"Logging configuration to be set for the server\"\"\"\n\n"
        },
        {
            "id": "chunk_1_LogConfig_2",
            "code": "    LOGGER_NAME: str = \"marketplace\"\n    LOG_FORMAT: str = \"%(levelprefix)s | %(asctime)s | %(message)s\"\n    LOG_LEVEL: str = \"DEBUG\"\n"
        },
        {
            "id": "chunk_1_LogConfig_3",
            "code": "\n"
        },
        {
            "id": "chunk_1_LogConfig_4",
            "code": "    # Logging config\n    version: int = 1\n    disable_existing_loggers: bool = False\n"
        },
        {
            "id": "chunk_1_LogConfig_5",
            "code": "    formatters: dict = {\n        \"default\": {\n            \"()\": \"uvicorn.logging.DefaultFormatter\",\n            \"fmt\": LOG_FORMAT,\n            \"datefmt\": \"%Y-%m-%d %H:%M:%S\",\n        },\n    }\n"
        },
        {
            "id": "chunk_1_LogConfig_6",
            "code": "    handlers: dict = {\n        \"default\": {\n            \"formatter\": \"default\",\n            \"class\": \"logging.StreamHandler\",\n            \"stream\": \"ext://sys.stderr\",\n        },\n    }\n"
        },
        {
            "id": "chunk_1_LogConfig_7",
            "code": "    loggers: dict = {\n        LOGGER_NAME: {\"handlers\": [\"default\"], \"level\": LOG_LEVEL},\n    }\n"
        },
        {
            "id": "chunk_1_LogConfig_8",
            "code": "\n\n"
        }
    ],
