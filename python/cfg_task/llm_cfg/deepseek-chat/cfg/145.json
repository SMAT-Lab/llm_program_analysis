        {
            "id": "chunk_3_BlackJackCard_1",
            "code": "class BlackJackCard(Card):\n\n"
        },
        {
            "id": "chunk_3_BlackJackCard_2",
            "code": "    def __init__(self, value, suit):\n        super(BlackJackCard, self).__init__(value, suit)\n\n"
        },
        {
            "id": "chunk_3_BlackJackCard_3",
            "code": "    def is_ace(self):\n        return True if self._value == 1 else False\n\n"
        },
        {
            "id": "chunk_3_BlackJackCard_4",
            "code": "    def is_face_card(self):\n        \"\"\"Jack = 11, Queen = 12, King = 13\"\"\"\n        return True if 10 < self._value <= 13 else False\n\n"
        },
        {
            "id": "chunk_3_BlackJackCard_5",
            "code": "    @property\n    def value(self):\n        if self.is_ace() == 1:\n            return 1\n        elif self.is_face_card():\n            return 10\n        else:\n            return self._value\n\n"
        },
        {
            "id": "chunk_3_BlackJackCard_6",
            "code": "    @value.setter\n    def value(self, new_value):\n        if 1 <= new_value <= 13:\n            self._value = new_value\n        else:\n            raise ValueError('Invalid card value: {}'.format(new_value))\n\n"
        },
        {
            "id": "chunk_4_Hand_1",
            "code": "class Hand(object):\n\n"
        },
        {
            "id": "chunk_4_Hand_2",
            "code": "    def __init__(self, cards):\n        self.cards = cards\n"
        },
        {
            "id": "chunk_4_Hand_3",
            "code": "    def add_card(self, card):\n        self.cards.append(card)\n"
        },
        {
            "id": "chunk_4_Hand_4",
            "code": "    def score(self):\n        total_value = 0\n        for card in self.cards:\n            total_value += card.value\n        return total_value\n"
        },
