        {
            "id": "chunk_1_HashTableClass_1",
            "code": "class HashTable(object):\n\n"
        },
        {
            "id": "chunk_1_HashTableClass_2",
            "code": "    def __init__(self, size):\n        self.size = size\n        self.table = [[] for _ in range(self.size)]\n"
        },
        {
            "id": "chunk_1_HashTableClass_3",
            "code": "    def _hash_function(self, key):\n        return key % self.size\n"
        },
        {
            "id": "chunk_1_HashTableClass_4",
            "code": "    def set(self, key, value):\n        hash_index = self._hash_function(key)\n        for item in self.table[hash_index]:\n            if item.key == key:\n                item.value = value\n                return\n        self.table[hash_index].append(Item(key, value))\n"
        },
        {
            "id": "chunk_1_HashTableClass_5",
            "code": "    def get(self, key):\n        hash_index = self._hash_function(key)\n        for item in self.table[hash_index]:\n            if item.key == key:\n                return item.value\n        raise KeyError('Key not found')\n"
        },
        {
            "id": "chunk_1_HashTableClass_6",
            "code": "    def remove(self, key):\n        hash_index = self._hash_function(key)\n        for index, item in enumerate(self.table[hash_index]):\n            if item.key == key:\n                del self.table[hash_index][index]\n                return\n        raise KeyError('Key not found')\n"
        },
